{
    "name": "h3x",
    "version": "1.0.0",
    "msteams": {
        "teamsAppId": null
    },
    "description": "Weather Agent with Microsoft 365 Agents SDK and LangChain",
    "engines": {
        "node": "18 || 20 || 22"
    },
    "author": "Microsoft",
    "license": "MIT",
    "main": "./src/index.js",
    "scripts": {
        "dev:teamsfx": "env-cmd --silent -f .localConfigs npm run dev",
        "dev:teamsfx:playground": "env-cmd --silent -f .localConfigs.playground npm run dev",
        "dev:teamsfx:launch-playground": "env-cmd --silent -f env/.env.playground teamsapptester start",
        "dev": "nodemon --inspect=9239 --signal SIGINT ./src/index.js",
        "start": "node ./src/index.js",
        "standalone": "env-cmd --silent -f env/.env.standalone npm run dev",
        "standalone:start": "env-cmd --silent -f env/.env.standalone npm start",
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch": "nodemon --exec \"npm run start\"",
        "setup-check": "node setup-check.js",
        "quick-start": "npm run setup-check && echo \"Starting playground...\" && npm run dev:teamsfx:playground",
        "standalone-start": "npm run setup-check && echo \"Starting standalone server...\" && npm run standalone"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com"
    },
    "dependencies": {
        "@azure/openai": "^2.0.0",
        "@langchain/langgraph": "^0.2.66",
        "@langchain/openai": "^0.5.6",
        "@microsoft/agents-hosting": "^0.2.14",
        "express": "^5.0.1"
    },
    "devDependencies": {
        "env-cmd": "^10.1.0",
        "nodemon": "^3.1.7",
        "shx": "^0.3.3"
    }
}
