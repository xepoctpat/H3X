# No-OpenAI Standalone Environment Configuration
TEAMSFX_ENV=standalone
NODE_ENV=development

# Framework Configuration
USE_OPENAI=false
USE_NATIVE_SDK=true
FRAMEWORK_VERSION=1.0.0

# Bot Framework Configuration
BOT_ID=local-sir-interface-$(date +%s)
BOT_PASSWORD=local-dev-password-123
PORT=3978

# Real-Life Standards Configuration
ENABLE_RL_STANDARDS=true
ENABLE_MONITORING=true
ENABLE_HUMAN_SUPERVISION=true


# GitHub Integration
GITHUB_TOKEN=your_github_token_here